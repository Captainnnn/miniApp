<view class="container"><text class="title">{{''+meetingRoomInfo.title+''}}</text><image class="meetingRoomImg" src="{{meetingRoomInfo.imgSrc}}"></image><view class="personAndScore"><text class="maxPerson">{{"座位数："+meetingRoomInfo.maxPerson}}</text><text class="score">{{meetingRoomInfo.needScore+"积分/0.5h"}}</text></view><text class="address">{{"地址："+meetingRoomInfo.address}}</text><view class="feature"><block wx:for="{{meetingRoomInfo.feature}}" wx:for-item="fe" wx:for-index="__i0__" wx:key="*this"><view class="featureItem">{{''+fe+''}}</view></block></view><view class="article">会议主题：<input class="inputArticle" type="text" placeholder="主题"/></view><view class="describe">会议描述：<textarea type="text" placeholder="描述"></textarea></view><view class="selectDate">选择日期：<picker mode="date" value="{{date}}" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e">{{date}}</picker></view><view class="speaker">主持人：<input type="text" placeholder="主持人"/></view><button class="advance" plain="true">立即预定</button></view>