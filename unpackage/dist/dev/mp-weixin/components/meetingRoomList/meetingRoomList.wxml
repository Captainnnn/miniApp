<view><block wx:for="{{meetingRoomList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="imgSrc"><view class="meetingRoomItem" data-meetingRoomId="{{item.imgSrc}}" data-event-opts="{{[['tap',[['meetingRoomDetail',['$event']]]]]}}" bindtap="__e"><view class="roomInfo"><image class="roomImg" src="{{item.imgSrc}}"></image><view class="baseInfo"><text class="title">{{item.title}}</text><text class="maxPerson">{{"坐席"+item.maxPerson+"人"}}</text><view class="feature"><block wx:for="{{item.feature}}" wx:for-item="fe" wx:for-index="__i1__" wx:key="*this"><view class="featureItem">{{''+fe+''}}</view></block></view><text class="needScore">{{"所需积分："+item.needScore+"/0.5h"}}</text></view></view><text class="address">{{"会议室地点："+item.address}}</text><view class="_hr"></view><text class="roomDetail">点击查看详情>></text></view></block></view>